//Learning pair of 2 elements in containers of STL
#include <bits/stdc++.h> // For competitive programming convenience
using namespace std;     // Use the standard namespace

void printPair()
{
    pair<int, int> p = {1, 3}; // Create a pair
    cout << p.first << " " << p.second << endl; // Access and print pair elements
}

int main() 
{
    printPair(); // Call the function to test
    return 0;
}
